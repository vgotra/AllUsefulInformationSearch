{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Auis": "Information"
    }
  },
  "ConnectionStrings": {
    "Auis_Wikipedia": "Server=server;Database=Auis_Wikipedia;Trusted_Connection=true;MultipleActiveResultSets=true;"
